config_version=5

[application]

config/name="DiceDeckRandom"
run/main_scene="res://scenes/title/title_screen.tscn"
config/features=PackedStringArray("4.6", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://autoload/game_manager.gd"
CardDatabase="*res://autoload/card_database.gd"
FirebaseManager="*res://autoload/firebase_manager.gd"
MultiplayerManager="*res://autoload/multiplayer_manager.gd"
EffectManager="*res://autoload/effect_manager.gd"

[display]

window/size/viewport_width=1080
window/size/viewport_height=1920
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_width"
window/handheld/orientation=1

[physics]

3d/physics_engine="Jolt Physics"

[gui]

theme/custom="res://fonts/default_theme.tres"



[rendering]

rendering_device/driver.windows="d3d12"
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true

